{
  "name": "@reliverse/native-app-example",
  "private": true,
  "version": "0.1.0",
  "description": "Example native CLI application built with Dler",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "native-app": "src/index.ts"
  },
  "scripts": {
    "build": "bun dler build --compile --production --target bun --format cjs --bytecode",
    "dev": "bun dler build --watch --target bun --format cjs",
    "start": "node dist/index.js"
  },
  "keywords": [
    "cli",
    "native",
    "executable",
    "dler",
    "bun"
  ],
  "author": "reliverse",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@p/tsconfig": "workspace:*",
    "@types/bun": "catalog:"
  }
}
