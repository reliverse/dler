    "clean": "bun dler clean --presets build --force",
    "typecheck": "bun dler tsc --copy-logs",
    "check": "bun format && bun typecheck",
    "format": "bun biome check --write --unsafe",
    "exports-to-ts": "bun scripts/src/replace-exports.ts --direction js-to-ts",
    "exports-to-js": "bun scripts/src/replace-exports.ts --direction ts-to-js",
    "latest": "bun dler update && bun install && bun check",
    "example:cli": "bun examples/cli-app/src/cli.ts"
