{
  "name": "@reliverse/dler",
  "author": "reliverse",
  "version": "1.7.10",
  "type": "module",
  "license": "MIT",
  "description": "@reliverse/dler is a flexible, unified, and fully automated bundler for typescript and javascript projects, as well as an npm and jsr publishing tool. dler is not only a bundler, it also tries to serve as the most powerful codemod toolkit for js/ts.",
  "scripts": {
    "dler": "bun src/cli.ts",
    "dev": "bun src/cli.ts --dev",
    "cmds": "bun src/cli.ts rempts",
    "cmd": "bun src/cli.ts rempts --init",
    "mock:bootstrap": "bun src/cli.ts mock",
    "mock:cleanup": "bun src/cli.ts mock --cleanup",
    "mock:merge": "bun src/cli.ts merge --dev --s src/templates --d src/app/mock/mock.ts --as-template",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --fix --unsafe . && knip",
    "latest": "bun update --latest && bun check",
    "pub": "bun src/cli.ts pub --dev",
    "tests": "bun test ./.tests"
  },
  "dependencies": {
    "@reliverse/bleump": "^1.1.4",
    "@reliverse/pathkit": "^1.2.1",
    "@reliverse/reglob": "^1.0.0",
    "@reliverse/relico": "^1.1.2",
    "@reliverse/relifso": "^1.4.5",
    "@reliverse/relinka": "^1.4.7",
    "@reliverse/rempts": "^1.7.17",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/pluginutils": "^5.1.4",
    "autoprefixer": "^10.4.21",
    "bun": "^1.2.15",
    "cssnano": "^7.0.7",
    "defu": "^6.1.4",
    "esbuild": "^0.25.5",
    "execa": "^9.6.0",
    "file-type": "^21.0.0",
    "fix-dts-default-cjs-exports": "^1.0.1",
    "hookable": "^5.5.3",
    "jiti": "^2.4.2",
    "magic-string": "^0.30.17",
    "mlly": "^1.7.4",
    "p-all": "^5.0.0",
    "p-map": "^7.0.3",
    "pkg-types": "^2.1.0",
    "postcss": "^8.5.4",
    "postcss-nested": "^7.0.2",
    "pretty-bytes": "^7.0.0",
    "pretty-ms": "^9.2.0",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "scule": "^1.3.0",
    "semver": "^7.7.2",
    "tinyglobby": "^0.2.14",
    "ts-morph": "^26.0.0",
    "untyped": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.28.0",
    "@reliverse/rse": "^1.7.3",
    "@stylistic/eslint-plugin": "^4.4.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.15",
    "@types/node": "^22.15.29",
    "eslint": "^9.28.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.14.0",
    "knip": "^5.59.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "@types/semver": "^7.7.0",
    "@volar/typescript": "^2.4.14",
    "@vue/language-core": "^2.2.10",
    "@vue/language-core2.0": "npm:@vue/language-core@2.2.10",
    "sass": "^1.89.1",
    "vue": "^3.5.16",
    "vue-sfc-transformer": "^0.1.16",
    "vue-tsc": "^2.2.10",
    "vue-tsc1": "npm:vue-tsc@^2.2.10",
    "vue-tsc2.0": "npm:vue-tsc@2.2.10"
  }
}
