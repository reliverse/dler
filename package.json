{
  "name": "@reliverse/dler",
  "author": "reliverse",
  "version": "1.7.111",
  "type": "module",
  "license": "MIT",
  "description": "The TypeScript framework for building JavaScript NPM libraries. @reliverse/dler is a flexible, unified, and fully automated bundler for typescript and javascript projects, as well as an npm and jsr publishing tool. dler is not only a bundler, it also tries to serve as the most powerful codemod toolkit for js/ts.",
  "engines": {
    "bun": ">=1.2.21",
    "node": ">=v22.18.0"
  },
  "scripts": {
    "dev": "bun dler",
    "pub": "bun dler pub --dev",
    "dler": "bun src-ts/dler.ts",
    "rse:cli": "bun rse cli",
    "release": "bun release.ts",
    "latest": "bun dler update && bun install",
    "build:binary": "bun dler build binary --input src-ts/dler.ts",
    "remdn": "bun dler remdn --mode dirs-scan-compare",
    "magic:example": "bun examples/ts/magic/e-ms-inject.ts",
    "magic": "bun dler magic",
    "tests": "bun test ./.tests",
    "cmds": "bun dler rempts",
    "cmd": "bun dler rempts --init",
    "check": "tsc --noEmit && biome check --fix --unsafe",
    "libs:pack": "bun dler pack dist-libs --output examples/ts/app/libs/mock",
    "libs:unpack": "bun dler pack examples/ts/app/libs/mock/mock-mod.ts --output dist-libs --unpack",
    "libs:example": "bun examples/ts/e-mod.ts libs",
    "stack:pack": "bun dler pack pack-input --output pack-output/better-t-stack",
    "stack:unpack": "bun dler pack pack-output/better-t-stack/better-t-stack-mod.ts --output unpacked-output/better-t-stack --unpack"
  },
