{
  "name": "@reliverse/dler-config",
  "description": "Configuration management for @reliverse/dler",
  "author": "reliverse",
  "version": "2.0.27",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/mod.ts",
      "default": "./src/mod.ts"
    },
    "./impl/*": {
      "types": "./src/impl/*.ts",
      "default": "./src/impl/*.ts"
    }
  },
  "dependencies": {
    "c12": "catalog:",
    "semver": "catalog:",
    "@reliverse/dler-pkg-tsc": "workspace:*"
  },
  "keywords": [
    "dler",
    "config",
    "build",
    "publish"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "license": "MIT",
  "devDependencies": {
    "@reliverse/dler-logger": "workspace:*",
    "@reliverse/dler-tsconfig": "workspace:*",
    "@microsoft/api-extractor": "catalog:",
    "typescript": "catalog:",
    "@types/bun": "catalog:",
    "@types/semver": "catalog:"
  }
}