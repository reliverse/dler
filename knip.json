{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": [
    "**/*.{ts,js}"
  ],
  "entry": [
    "src/app/**/*.ts",
    ".{config,tests}/**/*.ts",
    "*.{config,cfg}.{ts,js}",
    "src/mod.ts",
    "src/**/*-mod.ts",
    "example/**/*.ts"
  ],
  "ignore": [
    "reliverse.ts",
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/impl/**/*.ts",
    "src/libs/cfg/types.ts"
  ],
  "ignoreDependencies": [
    "octokit",
    "querystring",
    "bun-plugin-tailwind",
    "@hookform/resolvers",
    "@radix-ui/react-label",
    "@radix-ui/react-select",
    "@radix-ui/react-slot",
    "class-variance-authority",
    "clsx",
    "lucide-react",
    "react-hook-form",
    "tailwind-merge",
    "tailwindcss-animate",
    "autoprefixer",
    "postcss"
  ]
}