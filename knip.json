{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    "src/app/**/*.ts",
    ".config/**/*.ts",
    "*.{config,cfg}.{ts,js}",
    "src/mod.ts",
    "src/**/*-mod.ts",
    "src/cli/args/**/*.ts",
    "example/**/*.ts"
  ],
  "ignore": [
    ".config/rse.ts",
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/libs/sdk/sdk-impl/**/*.ts"
  ],
  "ignoreDependencies": ["bun", "@reliverse/relico"]
}
